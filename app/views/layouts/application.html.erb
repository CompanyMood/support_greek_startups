<!DOCTYPE html>
<html>
<head>
  <title>SupportGreekStartups</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

    <!-- Insert ribbon -->
    <div class='support-greek-startups-left'>&nbsp;</div>

    <!-- Begin page content -->
    <div class="container">
      <%= yield %>
    </div>

    <!-- footer -->
    <footer class="footer">
      <div class="container text-right">
        <%= link_to 'https://www.company-mood.com' do %>
          <%= image_tag 'company_mood_logo@2.png', height: '45px' %>
        <% end %>
        <span class="with-love clearfix">Build with ❤️  from</span>
      </div>
    </footer>
</body>

</html>
